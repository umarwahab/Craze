}
.payment-options label {
    font-family: Work Sans;
    font-style: normal;
    font-weight: 500;
    font-size: 12px;
    line-height: 18px;
    color: #3581EA;
}
.payment-method input[type='radio']:after {
    width: 15px;
    height: 15px;
    border-radius: 15px;
    top: -2px;
    left: -1px;
    position: relative;
    background-color: #d1d3d1;
    content: '';
    display: inline-block;
    visibility: visible;
    border: 2px solid #a39e9e;
}
.payment-method .greey{
    font-family: Work Sans;
font-style: normal;
font-weight: bold !important;
font-size: 14px !important;
line-height: 24px !important;
color: #ADADAD !important;
}
.order-status .bluee{
    background: #3581EA !important;
    color: #fff !important;
}
.payment-options button {
    font-family: Work Sans;
    font-style: normal;
    font-weight: 500;
    font-size: 14px;
    line-height: 24px;
    color: #3581EA;
    border: 0;
    width: 20%;
    height: 38px;
    background: #EDF3FD;
    border-radius: 8px;
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 0 15px 0 0;
}
.payment-method input[type='radio']:checked:after {
    width: 15px;
    height: 15px;
    border-radius: 15px;
    top: -2px;
    left: -1px;
    position: relative;
    background-color: #3581EA;
    content: '';
    display: inline-block;
    visibility: visible;
    border: 2px solid #a39e9e;
}

.user-form .payment-method {
    border: 1px solid #D6D6D6;
    box-sizing: border-box;
    border-radius: 8px;
    margin: 30px 0;
    width: 100%;
}

.is-opacity{
    opacity: 0.3;
}
/* Responsive */
@media screen and (max-width:767px) {
    

.checkout-form {
    width: 100%;
    margin: 30px 0;
    display: block;
}
.user-form {
    width: auto;
    border: 1px solid #ccc;
    border-radius: 8px;
    margin: 0 10px;
}
.order-status {
    width: auto;
    margin: 30px 30px;
    padding: 10px;
    background: #FFFFFF;
    border: 1px solid #F5F5F5;
    box-sizing: border-box;
    box-shadow: 12px 20px 24px rgb(69 79 89 / 8%);
    border-radius: 8px;
    height: auto;
}
.order-status .button-container button {
    background: #F5F5F5;
    border-radius: 8px;
    border: none;
    width: 100%;
    height: 40px;
    padding: 8px 16px;
    font-family: Work Sans;
    font-style: normal;
    font-weight: 500;
    font-size: 14px;
    line-height: 24px;
    color: #ADADAD;
    margin: auto;
}
.user-form .content .shipping {
    width: auto;
}
.user-form .content .payment-option {
    width: auto;
}
}

/* checkout 2 responsive */
@media screen and (max-width:550px) {
    .user-form .shipping-method {
        display: block;
    }
    .checkout-form .shipping-method .shipping-info {
        width: 90%;
        margin: 20px 0;
    }
    .payment-options button {
        width: 90%;
        margin: 20px auto;
    }
    .payment-method .payment-options {
        display: block;
        
    }
}


@media screen and (max-width:480px) {
    .user-name {
        display: block;
        width: 100%;
        margin: 20px 0;
    }
    .user-name .col-1 {
        width: auto;
    }
    .user-name .col-1 label {
            margin: 10px 7px 5px 7px;
    }
    
    .user-name .col-2 {
        width: auto;
    }
    .user-name .col-2 label {
     margin: 10px 7px 5px 7px;
    }
    
    .user-security {
        display: block;
        width: 100%;
        margin: 10px 0;
    }
    .user-security .col-1 {
        width: auto;
    }
    .user-security .col-2 {
        width: auto;
    }
    .user-mail input[type="name"] {
        background: #FFFFFF;
        border: 1px solid #D6D6D6;
        box-sizing: border-box;
        border-radius: 8px;
        width: 98%;
        height: 48px;
        text-indent: 10px;
        display: block;
        margin: 10px 5px;
    }
    .user-mail label {
  
        margin: 10px 7px 5px 7px;
    }
    .user-security label {
        margin: 10px 7px 5px 7px;
    }
    .checkout-section .user-form .content-heading {
        display: block;
    }

    .content-heading button {
        width: 100%;
    }
    .content-heading h1 {
        margin: 20px 5px;
        text-align: center;
    }
    .user-action {
        display: block;
        width: 100%;
        margin: 30px 0;
    }
    .user-action .col-1 {
        display: flex;
        width: auto;
        align-items: center;
        margin: 20px 0;
    }
    .user-action .col-2 {
        display: flex;
        width: 100%;
    }
    .user-action .col-2 button {
        width: 100%;
    }
}