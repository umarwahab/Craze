.checkout-heading {
    display: flex;
    width: 95%;
    margin: auto;
    position: relative;
}
.checkout-section .checkout-heading h1{
font-family: Work Sans;
font-style: normal;
font-weight: 800;
font-size: 48px;
line-height: 56px;
letter-spacing: -0.04em;
color: #0C356F;
margin: 30px 0;
}

.checkout-section {
    margin: 20px 0;
